{
  "gitbook": ">3.0.0",
  "root": "./api",
  "plugins": [
    "edit-link",
    "-fontsettings",
    "-highlight",
    "github",
    "github-buttons"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/aurorafe/ppview/tree/master/docs/api",
      "label": "编辑此页"
    },
    "github": {
      "url": "https://github.com/aurorafe/ppview.git"
    },
    "github-buttons": {
      "repo": "aurorafe/ppview",
      "types": [
        "star",
        "watch"
      ],
      "size": "small"
    }
  },
  "links": {
    "sharing": {
      "facebook": false,
      "twitter": false
    }
  }
}
